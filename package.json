{
  "name": "eb-scripts",
  "description": "A CLI and scripts used at Echobind",
  "version": "0.0.0-development",
  "author": "Joe Previte @jsjoeio",
  "bin": {
    "eb-scripts": "./bin/run"
  },
  "bugs": "https://github.com/echobind/eb-scripts/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "hygen": "^5.0.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@semantic-release/git": "^7.0.17",
    "@semantic-release/npm": "^5.3.2",
    "@types/node": "^12.12.3",
    "globby": "^10",
    "semantic-release": "^15.13.28",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/echobind/eb-scripts",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "eb-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "echobind/eb-scripts",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "gen": "hygen",
    "semantic-release": "semantic-release"
  },
  "types": "lib/index.d.ts",
  "release": {
    "plugins": [
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
