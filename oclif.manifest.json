{
  "version": "1.0.0",
  "commands": {
    "generate": {
      "id": "generate",
      "description": "generates new files",
      "pluginName": "eb-scripts",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "$ eb-scripts generate react-component -n MyNewComponent -p src/components\n    Loaded templates: _templates\n    added: src/MyNewComponent.js\n"
      ],
      "flags": {
        "help": {
          "name": "help",
          "type": "boolean",
          "char": "h",
          "description": "show CLI help",
          "allowNo": false
        },
        "name": {
          "name": "name",
          "type": "option",
          "char": "n",
          "description": "name to print",
          "default": "MyNewComponent"
        },
        "path": {
          "name": "path",
          "type": "option",
          "char": "p",
          "description": "path to where you want the files to go"
        }
      },
      "args": [
        {
          "name": "templateName",
          "description": "the template you want to use",
          "required": true,
          "options": [
            "react-component",
            "react-typescript-component",
            "react-native-typescript-component",
            "react-native-typescript-screen",
            "react-native-e2e",
            "util-typescript"
          ]
        }
      ]
    },
    "init": {
      "id": "init",
      "description": "initializes project by installing `eb-scripts` and adding scripts to `package.json`",
      "pluginName": "eb-scripts",
      "pluginType": "core",
      "aliases": [],
      "examples": ["$ npx eb-scripts init react"],
      "flags": {
        "help": {
          "name": "help",
          "type": "boolean",
          "char": "h",
          "description": "show CLI help",
          "allowNo": false
        }
      },
      "args": [
        {
          "name": "project",
          "description": "The language or framework of the project.\nValid options: react,react-typescript,react-native-typescript",
          "required": true,
          "options": ["react", "react-typescript", "react-native-typescript"]
        }
      ]
    }
  }
}
