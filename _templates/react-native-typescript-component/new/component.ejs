---
to: <%= root %>/<%= path %>/<%= h.changeCase.pascal(name) %>/<%= h.changeCase.pascal(name) %>.tsx
---
<% const formattedName = h.changeCase.pascal(name) -%>
import React, { FC } from 'react';

import { Container } from '../Container';

interface Props {
  /** prop info */
  cool: boolean;
}

const DEFAULT_PROPS: Partial<Props> = {
  cool: true,
}

/**
 * Component docs: what does this component do?
 */
export const <%= formattedName %>: FC<Props> = (props = DEFAULT_PROPS) => {
  const { children } = props;

  return (
    <Container>
      {children}
    </Container>
  );
};

export default <%= formattedName %>;
